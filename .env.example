# SafeWalk Environment Configuration
# Copy this file to .env and update with your actual values

# API Configuration
EXPO_PUBLIC_API_URL=https://api.safewalk.app
EXPO_PUBLIC_API_VERSION=v1

# Authentication
EXPO_PUBLIC_GOOGLE_CLIENT_ID=your_google_client_id_here
EXPO_PUBLIC_APPLE_CLIENT_ID=your_apple_client_id_here

# Location Services
EXPO_PUBLIC_LOCATION_PERMISSION_MESSAGE=SafeWalk needs access to your location to keep you safe

# Firebase Configuration (if using Firebase)
# EXPO_PUBLIC_FIREBASE_API_KEY=
# EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN=
# EXPO_PUBLIC_FIREBASE_PROJECT_ID=

# Feature Flags
EXPO_PUBLIC_ENABLE_ANALYTICS=true
EXPO_PUBLIC_ENABLE_CRASH_REPORTING=true

# Environment
EXPO_PUBLIC_ENV=development
